        <file src="js/stateRestore.bulma.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/stateRestore.bulma.css" target="content\Content"/>
        <file src="css/stateRestore.bulma.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/stateRestore.bulma.min.css" target="content\Content"/>
        <file src="css/stateRestore.bulma.min.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="types/stateRestore.bulma.d.ts" target="content\Scripts"/>
        <file src="types/stateRestore.bulma.d.ts" target="contentFiles\any\any\wwwroot\js"/>
    </files>
</package>

